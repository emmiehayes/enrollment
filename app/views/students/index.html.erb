<h1>All Students</h1>

  <% @students.each do |student| %>
  <%=  link_to "#{student.name}", student_path(student) %>
  <%= link_to "Delete", student_path(student), method: "delete" %>
  <% end %>

<p><%= link_to "See All Students", students_path%></p>
<p><%= link_to "Create New Student", new_student_path%></p>
